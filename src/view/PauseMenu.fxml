<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuPage">
   <children>
      <TitledPane alignment="CENTER" animated="false" collapsible="false" contentDisplay="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" text="Menu">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="800.0">
               <children>
                  <Button fx:id="resumeButton" layoutX="99.0" layoutY="58.0" minWidth="400.0" mnemonicParsing="false" onAction="#resumeGame" text="Resume">
                     <font>
                        <Font name="Calibri" size="30.0" />
                     </font>
                  </Button>
                  <Button fx:id="mainMenuButton" layoutX="99.0" layoutY="233.0" minWidth="400.0" mnemonicParsing="false" onAction="#gotoMainMenu" text="Quit Game">
                     <font>
                        <Font name="Calibri" size="30.0" />
                     </font>
                  </Button>
                  <Button fx:id="scoreboardButton" layoutX="99.0" layoutY="146.0" minWidth="400.0" mnemonicParsing="false" onAction="#openScoreboard" text="Scoreboard">
                     <font>
                        <Font name="Calibri" size="30.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
        </content>
         <font>
            <Font size="18.0" />
         </font>
      </TitledPane>
   </children>
</AnchorPane>
